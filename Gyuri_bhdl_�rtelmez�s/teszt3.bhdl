composit main(in,out)
	switch sw01;
	led led01;	
	wire w01;
	wire w02;
	composit comp01;
	assign comp01 w01;
	assign comp01 w02;
	assign w02 comp01;
	assign led01 w01;
endcomposit;

composit comp01(in w01, w02,out w02)
	andgate and01;
	inverter inv01;
	wire w03
	assign and01 w01;
	assign and01 w02;
	assign w03 and01;
	assign inv01 w03;
	assign w02 inv01;
endcomposit;