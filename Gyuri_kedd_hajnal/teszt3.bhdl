composit comp01(in w01,w02,out w03)
	andgate and01;
	inverter inv01;
	assign and01 = w01;
	assign and01 = w02;
	assign inv01 = and01;
	assign w03 = inv01;
endcomposit;

composit main(in,out)
	switch sw01;
	led led01;	
	wire w01;
	wire w02;
	comp01(in sw01,w01,out w01);
	assign comp01 = w01;
	assign comp01 = w02;
	assign led01 = w01;
endcomposit;