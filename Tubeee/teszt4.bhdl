composit comp01(in w01,w02,out w03, w04)
	wire feedback;
	assign feedback = (!feedback & w01) | (feedback & w02);
	assign w03 = feedback;
	wire to_w04;
	assign to_w04 = feedback & w02;
	assign w04 = to_w04;
endcomposit;

composit main(in,out)
	switch sw01;
	generator gen01;
	led led01;	
	oscilloscope osc01(10);	
	comp01(in sw01,gen01,out led01,osc01);
endcomposit;